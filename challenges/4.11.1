#include <stdio.h>

int main(void) {
   enum GroceryItem {GR_APPLES, GR_BANANAS, GR_JUICE, GR_WATER};
   enum GroceryItem userItem;

   userItem = GR_APPLES;

   /* Your solution goes here  */
   
   switch(userItem){
      case GR_APPLES:
      case GR_BANANAS:
         printf("Fruit\n");
         break;
      case GR_JUICE:
