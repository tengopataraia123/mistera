#include <stdio.h>
#include <string.h>

int main(void) {
   char simonPattern[50];
   char userPattern[50];
   int userScore;
   int i;

   userScore = 0;

   scanf("%s", simonPattern);
   scanf("%s", userPattern);

   /* Your solution goes here  */
   
   for(int i=0;i<strlen(simonPattern);i++){
      if(userPattern[i] != simonPattern[i])  break;
      userScore++;
   }

   printf("userScore: %d\n", userScore);

   return 0;
}
