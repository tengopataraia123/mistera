#include <stdio.h>

int main(void) {
   const int NUM_POINTS = 4;
   int dataPoints[NUM_POINTS];
   int minVal;
   int i;

   scanf("%d", &minVal);

   for (i = 0; i < NUM_POINTS; ++i) {
      scanf("%d ", &(dataPoints[i]));
   }

   /* Your solution goes here  */
   for (i = 0; i < NUM_POINTS; ++i)
      if(dataPoints[i] < minVal)
         dataPoints[i] *= 2;

   for (i = 0; i < NUM_POINTS; ++i) {
      printf("%d ", dataPoints[i]);
   }
   printf("\n");

   return 0;
}
