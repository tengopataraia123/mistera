#include <stdio.h>
#include <string.h>

int main(void) {

   /* Type your code here. */
   
   char pass[20];
   
   scanf("%s",pass);
   
   for(int i=0;i<=strlen(pass);i++){
      switch(pass[i]){
         case 'i':
            pass[i] = '!';
            break;
         case 'a':
            pass[i] = '@';
            break;
         case 'm':
            pass[i] = 'M';
            break;
         case 'B':
            pass[i] = '8';
            break;
         case 'o':
            pass[i] = '.';
            break;
      }
   }
   
   strcat(pass,"q*s");
   
   printf("%s\n",pass);

   return 0;
}
