#include <stdio.h>

int main(void) {
   const int NUM_ELEMENTS = 20;         // Number of input integers
   int userVals[NUM_ELEMENTS];          // Array to hold the user's input integers

   /* Type your code here. */
   
   int numInts = 0;
   scanf("%d",&numInts);
   
   for(int i=0;i<numInts;i++){
      scanf("%d",&userVals[i]);
   }
   
   for(int i=numInts-1;i>=0;i--){
     printf("%d ",userVals[i]);
   }
   
   printf("\n");

   return 0;
}
