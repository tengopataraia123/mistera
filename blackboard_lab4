#include <stdio.h>

int main()
{
    int amount=0;
    printf("Enter the amount: ");
    scanf("%d",&amount);
    printf("20 bills: %d\n",amount/20);
    amount %= 20;
    printf("10 bills: %d\n",amount/10);
    amount %= 10;
    printf("5 bills: %d\n",amount/5);
    amount %= 5;
    printf("1 bills: %d\n",amount);
    return 0;
}


#include <stdio.h>

int main()
{
    int i=0,j;

    while(i<10){
        j=0;
        while(j<=i){
            printf("*");
            j++;
        }
        printf("\n");
        i++;
    }
    return 0;
}


#include <stdio.h>

int main()
{
    for(int i=0;i<10;i++){
        for(int j=0;j<=i;j++)
            printf("*");
        printf("\n");
    }
    return 0;
}


#include <stdio.h>

int main()
{
    printf("*\n");
    printf("**\n");
    printf("***\n");
    printf("****\n");
    printf("*****\n");
    printf("******\n");
    printf("*******\n");
    printf("********\n");
    printf("*********\n");
    printf(“**********\n”);
    return 0;
}



